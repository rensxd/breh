import java.awt.Color;
import java.awt.Dimension;
import javax.swing.JPanel;

@SuppressWarnings("serial")
public class MyContentPane extends JPanel {
	
	
	JPanel myPanel = new JPanel();

	public MyContentPane(){
		create();
	}
	
	
	public void create() {
		
		myPanel.setPreferredSize(new Dimension(300, 300));
		myPanel.setLayout(null);
		
		JPanel panel1 = new JPanel();
		panel1.setBackground(Color.RED);
		panel1.setPreferredSize(new Dimension(150, 150));
		
		JPanel panel2 = new JPanel();
		panel2.setBackground(Color.GREEN);
		panel2.setBounds(0, 0, 150, 150);

		JPanel panel3 = new JPanel();
		panel3.setBackground(Color.BLUE);
		panel3.setPreferredSize(new Dimension(150, 150));
		
		JPanel panel4 = new JPanel();
		panel4.setBackground(Color.YELLOW);
		panel4.setPreferredSize(new Dimension(150, 150));
		
		myPanel.add(panel1);
		myPanel.add(panel2);
		myPanel.add(panel3);
		myPanel.add(panel4);
		
		
	}
	
	
	
	
	
}
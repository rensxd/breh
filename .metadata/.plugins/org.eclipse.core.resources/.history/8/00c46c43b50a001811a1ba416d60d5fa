
public class Zoo {
		
	Animal[] animals;
	
	public Zoo(String naam, int x) {
		animals = new Animal[x];
	}

	public void addAnimal(Animal animal) {
		
		for (int t = 0; t < animals.length; t++) {
			animals[t] = animal;
		}
		
	}
	
	public void animalEat() {
		
		for(Animal a : animals) {
			a.Eat();
		}
		
	}
	
	

}
